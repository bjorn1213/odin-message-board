<html>

<head>
    <link rel="stylesheet" href="/styles.css">
    <title>Mini message board</title>
</head>

<body>

    <div class="message-container">
        <% messages.forEach((message)=> { %>
            <%- include('message', {message: message}); %>
                <% }); %>
    </div>
</body>

</html>